{
    "name": "frengky/php-wkhtmltox",
    "description": "Simple wkhtmltox wrapper for PHP",
    "keywords": ["php", "wkhtmltopdf", "wkhtmltoimage", "wkhtmltox", "html", "pdf", "image"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/frengky/php-wkhtmltox",
    "authors": [
        {
            "name": "Frengky",
            "email": "frengky.lim@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Frengky\\WkHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frengky\\WkHtml\\Tests\\": "tests/"
        }
    }
}
